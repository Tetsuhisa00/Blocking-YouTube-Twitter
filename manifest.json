
{
  "manifest_version": 3,
  "name": "Blocking-YouTube-Twitter",
  "description": "This extension blocks YouTube and Twitter from your browser.",
  "version": "1.0.0",
  "permissions": ["activeTab", "webNavigation"],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}

